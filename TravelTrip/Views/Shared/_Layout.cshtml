
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title -->
    <title>@ViewBag.Title - Lifestyle Blog Template</title>
    <!-- Favicon -->
    <link rel="icon" href="~/original-master/img/core-img/favicon.ico">
    <!-- Style CSS -->
    <link rel="stylesheet" href="@Url.Content("~/original-master/style.css")">
</head>
<body>
    @Html.Action("Heading","PartialView")
    @RenderBody()
    @Html.Action("Instagram", "PartialView")
    <!-- jQuery (Necessary for All JavaScript Plugins) -->
    <script src="~/original-master/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="~/original-master/js/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/original-master/js/bootstrap.min.js"></script>
    <!-- Plugins js -->
    <script src="~/original-master/js/plugins.js"></script>
    <!-- Active js -->
    <script src="~/original-master/js/active.js"></script>
</body>
</html>
